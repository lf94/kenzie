//
// Global configuration parameters
//

// Extrusion height for 3D printers in millimeters.
// Uses the common height of 0.2mm.
extrude = 0.2;

// Extrusion width for 3D printers in millimeters.
// Use the common width of 0.4mm.
extrude_width = 0.4;

// Modes to indicate how to render the object.
RENDER_NORMAL = 0;
RENDER_EFFICIENT = 1;
render_mode = RENDER_EFFICIENT;

// Number of fragments to use when generating smooth surfaces.
$fn = 40;
